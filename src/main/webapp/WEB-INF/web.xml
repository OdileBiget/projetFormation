		<display-name>AjoutJardin</display-name>
		<description></description>
		<servlet-class>servlets.AjoutJardin</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>AfficherProfil</servlet-name>
		<display-name>AfficherProfil</display-name>
