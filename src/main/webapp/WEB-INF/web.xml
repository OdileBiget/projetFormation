	<servlet-mapping>
		<servlet-name>AjoutJardin</servlet-name>
		<url-pattern>/AjoutJardin</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>AfficherProfil</servlet-name>
		<url-pattern>/AfficherProfil</url-pattern>

	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>updateProfil</servlet-name>
		<url-pattern>/updateProfil</url-pattern>
